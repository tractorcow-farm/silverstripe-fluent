{
    "name": "tractorcow/silverstripe-fluent",
    "description": "Simple localisation for Silverstripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "localisation",
        "localization",
        "translation",
        "language",
        "locale",
        "multilingual",
        "translatable",
        "i18n",
        "internationalisation",
        "internationalization"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Damian Mooyman",
            "email": "damian.mooyman@gmail.com"
        },
        {
            "name": "Robbie Averill",
            "email": "robbie@silverstripe.com"
        }
    ],
    "support": {
        "issues": "http://github.com/tractorcow-farm/silverstripe-fluent/issues"
    },
    "require": {
        "php": "^7.1",
        "silverstripe/vendor-plugin": "^1",
        "silverstripe/framework": "^4",
        "silverstripe/admin": "^1",
        "silverstripe/versioned": "^1",
        "symbiote/silverstripe-gridfieldextensions": "^3.1",
        "ext-json": "*"
    },
    "suggest": {
        "silverstripe/cms": "Localise pages"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev",
            "5.x-dev": "5.1.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "TractorCow\\Fluent\\": "src/",
            "TractorCow\\Fluent\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "lint": "phpcs src/ tests/php/",
        "lint-clean": "phpcbf src/ tests/php/"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
